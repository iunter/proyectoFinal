<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alfa.alfacare">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".registrarPaciente" />
        <activity android:name=".turno" />
        <activity android:name=".seleccionHora" />
        <activity android:name=".verTurnos" />
        <activity android:name=".verUnTurno" />
        <activity android:name=".iniciarSesion" />
        <activity android:name=".modificarPaciente" />
        <activity android:name=".agregarFamiliar" />
        <activity android:name=".chat" />
        <activity android:name=".verChats" />
        <activity
            android:name=".Main2Activity"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".verUsuario" />
        <activity android:name=".muroActivity" />

        <service
            android:name=".MyIntentService"
            android:enabled="true"
            android:exported="false" />
        <service
            android:name=".PublicacionService"
            android:exported="false" />

        <activity
            android:name=".verChats2"
            android:label="@string/title_activity_ver_chats2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".NuevoChat"></activity>
    </application>

</manifest>